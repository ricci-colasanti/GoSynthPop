package main

import (
	"fmt"
	"math/rand"
	"sync"
	"time"
)

func count(id int, test []int, wg *sync.WaitGroup) {
	//defer wg.Done()
	for i := 0; i < 3; i++ {
		fmt.Printf("Process %d loop % d form array %d \n", id, i, test[rand.Intn(5)])
		time.Sleep(1 * time.Second)
	}
	wg.Done()
}
func main() {
	testArray := []int{1, 2, 3, 4, 5}
	var wg sync.WaitGroup
	for i := 0; i < 4; i++ {
		wg.Add(1)
		go count(i, testArray, &wg)
	}
	wg.Wait()
	fmt.Println("done")
}
